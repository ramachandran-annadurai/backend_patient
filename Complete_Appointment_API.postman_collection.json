					"        method: 'POST',",
					"        header: {",
					"            'Content-Type': 'application/json'",
					"        },",
					"        body: {",
					"            mode: 'raw',",
					"            raw: JSON.stringify({",
					"                login_identifier: 'patient@example.com',",
					"                password: 'password123'",
					"            })",
					"        }",
					"    }, function (err, response) {",
					"        if (response.json() && response.json().access_token) {",
					"            pm.environment.set('jwt_token', response.json().access_token);",
