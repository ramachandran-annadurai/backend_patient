							"raw": "{\n  \"week\": 20,\n  \"trimester\": 2,\n  \"tracking_date\": \"2024-01-15T10:00:00Z\",\n  \"symptoms_logged\": [\"back pain\", \"fatigue\", \"heartburn\"],\n  \"weight\": 65.5,\n  \"notes\": \"Feeling good overall, some back pain in the evening\",\n  \"mood\": \"happy\",\n  \"energy_level\": 7,\n  \"sleep_hours\": 8.5,\n  \"exercise_minutes\": 30,\n  \"water_intake\": 2.5\n}"
