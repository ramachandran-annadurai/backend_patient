	"event": [
		{
			"listen": "prerequest",
			"script": {
				"type": "text/javascript",
				"exec": [
					"// Auto-generate test data",
					"if (!pm.environment.get('auth_token') || pm.environment.get('auth_token') === 'your-jwt-token-here') {",
					"    pm.environment.set('auth_token', 'test-token-' + Date.now());",
					"}",
					"",
					"// Set random pregnancy week for testing",
					"const weeks = [8, 12, 20, 24, 28, 32, 36];",
					"const randomWeek = weeks[Math.floor(Math.random() * weeks.length)];",
					"pm.environment.set('pregnancy_week', randomWeek.toString());"
				]
			}
		},
		{
			"listen": "test",
			"script": {
				"type": "text/javascript",
				"exec": [
					"// Basic response validation",
					"pm.test('Response status code is 200 or 401', function () {",
					"    pm.expect(pm.response.code).to.be.oneOf([200, 401]);",
					"});",
					"",
					"// Check response format",
					"if (pm.response.code === 200) {",
					"    pm.test('Response has success field', function () {",
					"        const jsonData = pm.response.json();",
					"        pm.expect(jsonData).to.have.property('success');",
					"    });",
					"    ",
					"    pm.test('Response time is less than 5000ms', function () {",
					"        pm.expect(pm.response.responseTime).to.be.below(5000);",
					"    });",
					"}"
				]
			}
		}
	]
